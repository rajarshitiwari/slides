source "https://rubygems.org"

gem "jekyll", "~> 4.3"
gem "webrick", "~> 1.8"   # needed for `jekyll serve` on Ruby 3+

gem "jekyll-paginate"     # if you ever want pagination
gem "jekyll-include-cache" # helps if you use includes heavily
gem "jekyll-sitemap"      # generates sitemap.xml (good for indexing)
